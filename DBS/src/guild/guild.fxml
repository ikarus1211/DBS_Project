<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="guild.GuildController">
    <children>
        <ImageView fitHeight="469.0" fitWidth="622.0" layoutX="-12.0" layoutY="-13.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../img/lightbrown.jpg" />
            </image>
        </ImageView>
        <Button fx:id="bckBtn" layoutX="438.0" layoutY="352.0" mnemonicParsing="false" onAction="#goBack" prefHeight="38.0" prefWidth="121.0" text="Back">
         <font>
            <Font size="18.0" />
         </font></Button>
        <Label fx:id="name" layoutX="53.0" layoutY="66.0" prefHeight="47.0" prefWidth="518.0" text="Guild Name">
         <font>
            <Font name="Centaur" size="30.0" />
         </font></Label>
        <Label fx:id="leader" layoutX="53.0" layoutY="121.0" prefHeight="62.0" prefWidth="518.0" text="Leader">
         <font>
            <Font name="Centaur" size="30.0" />
         </font></Label>
        <Label fx:id="rank" layoutX="53.0" layoutY="188.0" prefHeight="53.0" prefWidth="262.0" text="Rank">
         <font>
            <Font name="Centaur" size="30.0" />
         </font></Label>
        <Label fx:id="no_players" layoutX="53.0" layoutY="250.0" text="Number of members">
         <font>
            <Font name="Centaur" size="30.0" />
         </font></Label>
    </children>
</AnchorPane>
